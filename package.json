{
    "name": "asset-forge",
    "version": "1.0.0",
    "description": "RWA Creator Studio - No-code platform for tokenizing real-world assets on Mantle",
    "private": true,
    "workspaces": [
        "frontend",
        "contracts"
    ],
    "scripts": {
        "dev": "npm run dev --workspace=frontend",
        "build": "npm run build --workspace=frontend",
        "test": "npm run test --workspace=contracts",
        "compile": "npm run compile --workspace=contracts",
        "deploy:sepolia": "npm run deploy:sepolia --workspace=contracts",
        "install:all": "npm install && cd contracts && npm install && cd ../frontend && npm install"
    },
    "keywords": [
        "rwa",
        "tokenization",
        "mantle",
        "blockchain",
        "real-world-assets"
    ],
    "author": "",
    "license": "MIT"
}